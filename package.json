{
    "name": "artemis",
    "version": "0.4.0",
    "private": true,
    "description": "Collection of open-source tools to quickly prototype and test mobile AR scenes",
    "scripts": {
        "install:viewer": "cd viewer && npm install",
        "install:server": "cd server && npm install",
        "postinstall": "npm-run-all -s install:*",
        "start:viewer": "cd viewer && npm run build",
        "start:server": "cd server && npm start",
        "start": "npm-run-all -s start:*",
        "dev:viewer": "cd viewer && npm run watch",
        "dev:server": "cd server && npm run dev",
        "dev": "npm-run-all -p dev:*"
    },
    "author": "Julien Letellier",
    "license": "MIT",
    "dependencies": {
        "npm-run-all": "^4.1.5"
    }
}
